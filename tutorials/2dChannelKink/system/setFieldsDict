FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 1
    volScalarFieldValue alpha.particles 0
);

regions
(
    boxToCell
    {
        box (-0.045 0 -0.1) (-0.0425 0.5 0.1);
        fieldValues
        (
            volScalarFieldValue alpha.water 0.65
            volScalarFieldValue alpha.particles 0.35
        );
    }
);


