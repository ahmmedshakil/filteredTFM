FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [1 -1 -2 0 0 0 0];

internalField       uniform 0;

boundaryField
{
    inlet
    {
//        type            fixedFluxPressure;
        type cyclic;
        value           $internalField;
    }

    outlet
    {
//        type            prghPressure;
//        p               $internalField;
        type cyclic;
        value           $internalField;
    }

    "walls"
    {
        type            zeroGradient;
        value           $internalField;
    }

    "symm.*"
    {
//        type symmetryPlane;
        type empty;
    }


}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
